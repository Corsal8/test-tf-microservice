// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema
generator client {
    provider = "prisma-client"
    output   = "../src/generated/prisma"
}

datasource db {
    provider = "sqlserver"
}

model Code {
    id      Int    @id @default(autoincrement())
    name    String
    content String
}
